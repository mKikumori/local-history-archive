<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="1000.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.local_history_archive.HelloController">
   <children>
      <HBox layoutY="-2.0" prefHeight="70.0" prefWidth="1000.0">
         <children>
            <ImageView fitHeight="70.0" fitWidth="104.0" pickOnBounds="true" preserveRatio="true" />
            <TextField promptText="Looking for something?">
               <HBox.margin>
                  <Insets left="70.0" top="20.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="3.0" left="3.0" right="50.0" top="3.0" />
               </padding>
            </TextField>
            <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets left="5.0" top="15.0" />
               </HBox.margin>
            </ImageView>
            <Label text="Upload" textAlignment="CENTER">
               <HBox.margin>
                  <Insets left="170.0" top="20.0" />
               </HBox.margin>
            </Label>
            <Label text="Colletions" textAlignment="CENTER">
               <HBox.margin>
                  <Insets left="20.0" top="20.0" />
               </HBox.margin>
            </Label>
            <Label text="Settings" textAlignment="CENTER">
               <HBox.margin>
                  <Insets left="20.0" top="20.0" />
               </HBox.margin>
            </Label>
            <Button mnemonicParsing="false" text="Sign In">
               <HBox.margin>
                  <Insets left="60.0" top="20.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" text="Register">
               <HBox.margin>
                  <Insets left="20.0" top="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
